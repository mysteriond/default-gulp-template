{
  "name": "default-gulp-template",
  "description": "My initial gulp template",
  "main": "index.js",
  "authors": [
    "mysterion141"
  ],
  "license": "ISC",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "overrides": {
    "bootstrap": {
      "main": [
        "./dist/js/bootstrap.min.js",
        "./dist/css/bootstrap.min.css",
        "./dist/css/bootstrap-theme.min.css",
        "./dist/fonts/*.*"
      ]
    },
    "susy": {
      "ignore": true
    },
    "breakpoint-sass": {
      "ignore": true
    },
    "html5shiv": {
      "main": "./dist/html5shiv.min.js"
    },
    "es5-shim": {
      "main": "./es5-shim.min.js"
    },
    "respond": {
      "main": "./dest/respond.min.js"
    },
    "jquery": {
      "main": "./jquery.min.js"
    },
    "angular": {
      "main": "./angular.min.js"
    }
  },
  "dependencies": {
    "jquery": "1.9.1",
    "reset-css": "^2.2.0",
    "respond": "^1.4.2",
    "html5shiv": "^3.7.3",
    "es5-shim": "^4.5.9",
    "breakpoint-sass": "^2.7.1",
    "bootstrap-sass-grid": "^4.0.0"
  }
}
